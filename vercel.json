{
    "version": 2,
    "name": "site-management",
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/index.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "functions": {
      "api/**/*.js": {
        "maxDuration": 60
      }
    }
  }
  